{
  "name": "pong-renderer",
  "version": "0.0.4",
  "description": "Pong Renderer",
  "main": "dist/lib.cjs.js",
  "module": "dist/lib.esm.js",
  "typings": "./dist/typings/Lib.d.ts",
  "scripts": {
    "clean": "rimraf ./dist",
    "build": "npm-run-all clean rollup:build typings:emit",
    "typings:emit": "tsc --emitDeclarationOnly true",
    "dev": "npm-run-all -s clean -p rollup:watch",
    "rollup:build": "cross-env NODE_ENV=production rollup -c",
    "rollup:watch": "cross-env NODE_ENV=production rollup -c -w",
    "test": "jest"
  },
  "jest": {
    "transform": {
      "^.+\\.tsx?$": "ts-jest"
    },
    "testRegex": "(/__tests__/.*|(\\.|/)(test|spec))\\.(jsx?|tsx?)$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json"
    ]
  },
  "author": "David Komer",
  "license": "MIT",
  "devDependencies": {
    "@types/gl-matrix": "^2.4.4",
    "@types/jest": "23.3.13",
    "@types/node": "10.12.18",
    "chokidar": "2.0.4",
    "cross-env": "5.2.0",
    "glob": "7.1.3",
    "jest": "^23.6.0",
    "jest-cli": "^23.6.0",
    "minify": "4.1.0",
    "npm-run-all": "4.1.5",
    "rimraf": "2.6.3",
    "rollup": "1.1.1",
    "rollup-plugin-commonjs": "9.2.0",
    "rollup-plugin-node-resolve": "4.0.0",
    "rollup-plugin-replace": "2.1.0",
    "rollup-plugin-string": "^2.0.2",
    "rollup-plugin-typescript2": "0.19.2",
    "rollup-plugin-uglify": "6.0.1",
    "shelljs": "0.8.3",
    "ts-jest": "^23.10.5",
    "ts-node": "7.0.1",
    "tslib": "1.9.3",
    "typescript": "3.2.4",
    "uglify-es": "3.3.10"
  },
  "peerDependencies": {},
  "dependencies": {
    "gl-matrix": "3.0.0",
    "webfontloader": "^1.6.28"
  }
}
